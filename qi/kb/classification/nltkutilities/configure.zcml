<configure
    xmlns="http://namespaces.zope.org/zope"
    i18n_domain="qi.kb.classification">

    <utility
        provides="..interfaces.ITokenizer"
        factory=".tokenizer.NLTKTokenizer"
        name="qi.kb.classification.tokenizers.NLTKTokenizer"
    />

    <utility
        provides="..interfaces.IPOSTagger"
        factory=".tagger.PennTreebankTagger"
        name="qi.kb.classification.taggers.PennTreebankTagger"
    />

    <utility
        provides="..interfaces.IPOSTagger"
        factory=".tagger.NgramTagger"
        name="qi.kb.classification.taggers.NgramTagger"
    />

</configure>
